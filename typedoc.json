{
  "out": "tsdoc",
  "githubPages": true,
  "disableSources": false,
  "exclude": ["**/lib/*.ts", "**/test/*.ts"],
  "basePath": "./src",
  "visibilityFilters": {},
  "intentionallyNotExported": [
    "ArangojsError",
    "ArangojsResponse",
    "Blob",
    "GeneratedAqlQuery"
  ],
  "excludeInternal": true,
  "excludeProtected": true,
  "excludePrivate": true,
  "includeVersion": false,
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}
