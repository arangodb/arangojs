{
  "basePath": "./src",
  "cacheBust": true,
  "disableGit": true,
  "disableSources": false,
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "exclude": ["**/lib/*.ts", "**/test/*.ts"],
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "githubPages": true,
  "includeVersion": false,
  "out": "tsdoc",
  "visibilityFilters": {},
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}
