{
  "name": "arangojs",
  "version": "6.0.0",
  "license": "Apache-2.0",
  "description": "The official ArangoDB JavaScript driver.",
  "homepage": "https://github.com/arangodb/arangojs",
  "bugs": {
    "url": "https://github.com/arangodb/arangojs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arangodb/arangojs.git"
  },
  "author": "ArangoDB GmbH",
  "contributors": [
    {
      "name": "Alan Plum",
      "email": "me@pluma.io"
    }
  ],
  "keywords": [
    "arango",
    "arangodb",
    "aql",
    "nosql",
    "client",
    "driver",
    "api",
    "http",
    "rest"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/arangojs.d.ts",
  "browser": {
    "./lib/cjs/util/btoa.js": "./lib/cjs/util/btoa.web.js",
    "./lib/cjs/util/bytelength.js": "./lib/cjs/util/bytelength.web.js",
    "./lib/cjs/util/joinPath.js": "./lib/cjs/util/joinPath.web.js",
    "./lib/cjs/util/multipart.js": "./lib/cjs/util/multipart.web.js",
    "./lib/cjs/util/request.js": "./lib/cjs/util/request.web.js",
    "./lib/esm/util/btoa.js": "./lib/esm/util/btoa.web.js",
    "./lib/esm/util/bytelength.js": "./lib/esm/util/bytelength.web.js",
    "./lib/esm/util/joinPath.js": "./lib/esm/util/joinPath.web.js",
    "./lib/esm/util/multipart.js": "./lib/esm/util/multipart.web.js",
    "./lib/esm/util/request.js": "./lib/esm/util/request.web.js"
  },
  "files": [
    "lib/web.*",
    "lib/cjs/*.*",
    "lib/esm/*.*",
    "lib/cjs/util/*.*",
    "lib/esm/util/*.*"
  ],
  "scripts": {
    "test": "mocha --growl --reporter spec --require source-map-support/register --require core-js/shim --timeout 10000 lib/cjs/test",
    "pretest": "rm -r lib/cjs ; npm run dist:cjs",
    "ci": "mocha --reporter spec --require source-map-support/register --require core-js/shim --timeout 10000 lib/cjs/test",
    "preci": "npm install",
    "jenkins": "npm run ci -- --timeout 0 --reporter xunit-file",
    "dist:cjs": "tsc -p . --outDir ./lib/cjs/ --module commonjs && cp src/index.cjs.js lib/cjs/index.js",
    "dist:esm": "tsc -p . --outDir ./lib/esm/ --module es2015 && cp src/index.esm.js lib/esm/index.js",
    "dist:web": "webpack",
    "dist": "rm -r lib ; npm-run-all --parallel dist:*",
    "prepublish": "npm run dist && node -e 'require(\".\");'"
  },
  "dependencies": {
    "es6-error": "^4.0.1",
    "multi-part": "^2.0.0",
    "utf8-length": "^0.0.1",
    "xhr": "^2.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.58",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.0.2",
    "core-js": "^2.4.1",
    "eslint": "^4.13.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "3.4.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.1",
    "regenerator-runtime": "^0.11.1",
    "source-map-support": "^0.5.0",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.0.0",
    "xunit-file": "1.0.0"
  }
}