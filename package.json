{
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "jenkins": "mocha --reporter spec --require source-map-support/register --timeout 0 build/test",
    "test": "mocha --reporter spec --require source-map-support/register --timeout 10000 build/test",
    "pretest": "yarn build",
    "build:package": "cp package-public.json build/package.json",
    "build:cjs": "tsc -p . --outDir ./build/ --target es2018 --declarationMap true && cp src/_public.js build/index.js",
    "build:web": "webpack",
    "build": "rm -rf build && mkdir build && npm-run-all --parallel build:*",
    "prepare": "yarn build && node -e 'require(\".\");'"
  },
  "dependencies": {
    "@types/node": ">=10",
    "es6-error": "^4.0.1",
    "multi-part": "^3.0.0",
    "x3-linkedlist": "1.1.0",
    "xhr": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/request-promise": "^4.1.45",
    "arangodb-instance-manager": "^1.2.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.13.5",
    "source-map-support": "^0.5.16",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "xunit-file": "^1.0.0"
  },
  "babel": {
    "plugins": [
      "@babel/proposal-class-properties"
    ],
    "presets": [
      "@babel/env",
      [
        "@babel/typescript",
        {
          "allowNamespaces": true
        }
      ]
    ]
  },
  "browserslist": [
    "> 2%",
    "ie 11"
  ]
}
